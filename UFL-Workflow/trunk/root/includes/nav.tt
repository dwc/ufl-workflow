<ul id="nav">
[% IF c.check_user_roles('Administrator') %]
  [% FOREACH name IN [ 'Users', 'Groups', 'Processes', 'Statuses' ] %]
    [% SET uri = c.uri_for(c.controller(name).action_for('index')) %]
  <li[% IF uri == c.req.uri.replace('\/+$', '') %] id="active"[% END %]><a href="[% uri | html %]">[% name | html %]</a></li>
  [% END %]
[% END %]
  <li><a href="[% c.uri_for(c.controller('Requests').action_for('index')) | html %]">Requests</a></li>
</ul>
